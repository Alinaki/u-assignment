{
  "name": "u-assignment",
  "version": "0.0.1",
  "private": true,
  "author": "Sergey Bekharsky",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alinaki/u-assignment"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "date-fns": "^2.0.1",
    "jsdoc-babel": "^0.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "recharts": "^1.7.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "doc": "jsdoc -c jsdoc.json"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "quotes": [
        2,
        "single",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "docdash": "^1.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jsdoc": "^3.6.3",
    "react-test-renderer": "^16.9.0"
  }
}
